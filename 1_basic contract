//The first line is for license identifier

//informing the compiler about the language and version of language 
pragma solidity >=0.7.0 <0.9.0;

//importing other contracts

//starting point of the contract
contract simple{

//variable declarations 
uint val;

//constructor area

//User defined function area
//getting data
function get(uint _val) public{
    val=_val;
}

//printing data
function put()public view returns(uint){
    return val;
}

}
